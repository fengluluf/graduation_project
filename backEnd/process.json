{
  "apps": [
    {
      "name": "app1",
      "script": "bin/www",
      "log_date_format": "YYYY-MM-DD HH:mm:SS",
      "log_file": "logs/app1.log",
      "error_file": "logs/app1-err.log",
      "out_file": "logs/app1-out.log",
      "pid_file": "pids/app1.pid",
      "instances": 1,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "1M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster_mode",
      "autorestart": true,
      "vizion": false
    }
  ]
}